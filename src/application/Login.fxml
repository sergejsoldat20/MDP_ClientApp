<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="application.MainController">
   <center>
      <AnchorPane prefHeight="316.0" prefWidth="380.0" BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="80.0" layoutY="93.0" prefHeight="130.0" prefWidth="219.0">
               <children>
                  <TextField fx:id="usernameField" prefHeight="30.0" prefWidth="219.0" promptText="username" />
                  <PasswordField fx:id="passwordField" prefHeight="30.0" prefWidth="219.0" promptText="password" />
                  <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginAction" prefHeight="30.0" prefWidth="219.0" text="Log in" />
               </children>
            </VBox>
            <Label alignment="CENTER" layoutX="82.0" layoutY="15.0" prefHeight="52.0" prefWidth="215.0" text="ZSMDP login">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
